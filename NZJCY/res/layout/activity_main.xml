 <RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#4b96e8"
    android:layout_gravity="center_horizontal">
    
      <ImageView 
	        android:id="@+id/logoIdImageView"
	        android:layout_marginTop="30dp"
	        android:layout_width="184dp"
	        android:layout_height="184dp"
	        android:src="@drawable/logo"
	        android:layout_centerHorizontal="true"/>
        
        <ImageButton 
            android:layout_toRightOf="@id/logoIdImageView"
            android:id="@+id/userloginImageButton"
            android:layout_marginTop="10dp"
            android:layout_width="39dp"
            android:layout_height="41dp"
            android:background="@drawable/headimage"
            android:layout_marginLeft="35dp"/>
     
<!--     <LinearLayout  -->
<!--         android:id="@+id/topLogoHeadImageLinearLayout" -->
<!--         android:layout_width="match_parent" -->
<!--         android:layout_height="214dp" -->
<!--         android:orientation="horizontal"> -->
        
       
        
